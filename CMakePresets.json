{
    "version": 5,
    "cmakeMinimumRequired": {
        "major": 3,
        "minor": 13,
        "patch": 0
    },
    "configurePresets": [
        {
            "name": "an386",
            "displayName": "ARMv7e-M",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/out/${presetName}",
            "toolchainFile": "toolchains/gcc-armv7e_m-unknown-none-eabi.toolchain",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "EXTRA_C_FLAGS": ""
            }
        }
    ],
    "buildPresets": [
        {
            "name": "an386",
            "configurePreset": "an386",
            "targets": [
                "all",
                "install"
            ]
        }
    ]
}